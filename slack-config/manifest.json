{
    "display_information": {
        "name": "prambook-local",
        "description": "prambook's description",
        "background_color": "#040f14"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": true,
            "messages_tab_read_only_enabled": false
        },
        "bot_user": {
            "display_name": "prambook-local",
            "always_online": true
        }
    },
    "oauth_config": {
        "redirect_urls": [
            "https://skylark-climbing-hermit.ngrok-free.app/oauth/access-token"
        ],
        "scopes": {
            "bot": [
                "chat:write",
                "im:history",
                "im:read",
                "im:write"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "request_url": "https://skylark-climbing-hermit.ngrok-free.app/event",
            "bot_events": [
                "message.im"
            ]
        },
        "org_deploy_enabled": false,
        "socket_mode_enabled": false,
        "token_rotation_enabled": false
    }
}